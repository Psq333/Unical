auto lo
iface lo inet loopback

auto eth0
iface eth0 inet static
    address 10.0.14.10
    netmask 255.255.255.252
    network 10.0.14.8
    broadcast 10.0.14.11
    gateway 10.0.14.11
    
    
auto eth1
iface eth1 inet static
    address 10.0.8.1
    netmask 255.255.254.0
    network 10.0.8.0
    broadcast 10.0.9.255


auto eth4
iface eth4 inet static
    address 10.0.10.1
    netmask 255.255.254.0
    network 10.0.10.0
    broadcast 10.0.11.255
    
    
auto eth2
iface eth2 inet static
    address 10.0.14.1 
    netmask 255.255.255.252
    network 10.0.14.0
    broadcast 10.0.14.3
